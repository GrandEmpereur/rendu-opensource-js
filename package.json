{
  "name": "rendu-opensource-js",
  "version": "1.0.0",
  "main": "main.ts",
  "author": "GrandEmpereur",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --files ./src/server.ts",
    "build": "rimraf ./build && yarn tsc",
    "start": "node --experimental-specifier-resolution=node ./build/server.js",
    "tsc": "tsc",
    "test": "jest --watchAll --runInBand --detectOpenHandles --forceExit",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "axios-mock-adapter": "^1.21.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "node-env": "^0.1.6",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@types/jest": "^29.5.2",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-ratelimit": "^0.9.3",
    "@types/koa__cors": "^3.1.1",
    "@types/node": "^17.0.24",
    "axios": "^1.4.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa2-ratelimit": "^1.1.3",
    "ts-jest": "^29.1.1"
  },
